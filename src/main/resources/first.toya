function sieve(n: int) -> boolean[] {
    var prime = new boolean[n+1]
    for(var i = 0; i <= n; i=i+1) {
        prime[i] = true
    }

    for(var p = 2; p*p <= n; p=p+1) {
        for(var i = p*p; i <=n; i=i+1) {
            prime[i] = false
        }
    }
    prime
}

function main(args: string[]) {
    var n = 500
    var primes = sieve(n)

    for(var i = 2; i <= n; i=i+1) {
        if(prime[i]) {
            println(i)
        }
    }
}